; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="Godot Projects"
run/main_scene="res://scenes/main.tscn"
config/features=PackedStringArray("4.4", "Forward Plus")
config/icon="res://icon.svg"

[autoload]

SignalBus="*res://signal_bus.gd"
Global="*res://global.gd"
DialogueManager="*res://addons/dialogue_manager/dialogue_manager.gd"

[debug]

gdscript/warnings/integer_division=0

[dialogue_manager]

editor/advanced/custom_test_scene_path="res://scenes/cutscenes/dialogue/balloon.tscn"
runtime/balloon_path="res://scenes/cutscenes/dialogue/balloon.tscn"

[display]

window/size/viewport_width=768
window/size/viewport_height=576
window/stretch/mode="viewport"

[editor_plugins]

enabled=PackedStringArray("res://addons/dialogue_manager/plugin.cfg", "res://addons/gut/plugin.cfg")

[file_customization]

folder_colors={
"res://battle/": "orange",
"res://data/": "yellow",
"res://scenes/": "green"
}

[global_group]

player=""

[gui]

fonts/dynamic_fonts/use_oversampling=false
theme/custom="res://scenes/themes/base.tres"
theme/default_font_antialiasing=0
theme/default_font_hinting=0
theme/default_font_subpixel_positioning=0
theme/lcd_subpixel_layout=0

[input]

cancel={
"deadzone": 0.2,
"events": [Object(InputEventKey,"resource_local_to_scene":false,"resource_name":"","device":-3,"window_id":0,"alt_pressed":false,"shift_pressed":false,"ctrl_pressed":false,"meta_pressed":false,"pressed":false,"keycode":0,"physical_keycode":88,"key_label":0,"unicode":120,"location":0,"echo":false,"script":null)
]
}
interact={
"deadzone": 0.2,
"events": [Object(InputEventKey,"resource_local_to_scene":false,"resource_name":"","device":-3,"window_id":0,"alt_pressed":false,"shift_pressed":false,"ctrl_pressed":false,"meta_pressed":false,"pressed":false,"keycode":0,"physical_keycode":90,"key_label":0,"unicode":122,"location":0,"echo":false,"script":null)
]
}

[internationalization]

locale/translations_pot_files=PackedStringArray("res://scenes/maps/town/dialogue/red_house.dialogue", "res://scenes/maps/town/dialogue/blue_house.dialogue", "res://scenes/maps/town/dialogue/town_sign.dialogue", "res://scenes/maps/town/dialogue/trainer_tips.dialogue", "res://scenes/maps/town/dialogue/girl_npc.dialogue", "res://scenes/maps/town/dialogue/oak_lab.dialogue")

[layer_names]

2d_physics/layer_1="Player"
2d_physics/layer_2="Static"
2d_physics/layer_3="InteractionFinder"

[rendering]

textures/canvas_textures/default_texture_filter=0
